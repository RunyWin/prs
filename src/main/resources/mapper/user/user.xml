<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wymessi.mapper.UserMapper">

	<insert id="registerApplicant" parameterType="applicant">
		<selectKey resultType="java.lang.Long" keyProperty="id"
			order="AFTER">
			SELECT LAST_INSERT_ID()
		</selectKey>
		INSERT INTO applicant(id,applicant_name,password,sex,mail,phone)
		VALUES(#{id},#{applicantName},#{password},#{sex},#{mail},#{phone})
	</insert>

</mapper>